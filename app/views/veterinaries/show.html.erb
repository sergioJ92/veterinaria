<%- model_class = Veterinary -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name("Nombre") %>:</strong></dt>
  <dd><%= @veterinary.name %></dd>
</dl>

<%= link_to t('.back', :default => t("Atras")),
              veterinaries_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("Editar")),
              edit_veterinary_path(@veterinary), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("Eliminar")),
              veterinary_path(@veterinary),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Seguro?')) },
              :class => 'btn btn-danger' %>
